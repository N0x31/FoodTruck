<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="fr.wcs.foodtruck.ChoixReserveContactAdmin">

    <TextView
        android:id="@+id/choixreservation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/choix_res"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="158dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.439"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/choixcontact"
        app:layout_constraintVertical_bias="0.136" />

    <TextView
        android:id="@+id/choixcontact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/choix_contact"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/choixreservation"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="293dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/choixreservation"
        app:layout_constraintVertical_bias="0.418"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/choixreservation" />
</android.support.constraint.ConstraintLayout>
